{
    "name": "iwouldrathercode/php-simple-ad-connect",
    "description": "LDAP wrapper for AD authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shankar Ganesh",
            "email": "psgganesh@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/iwouldrathercode/php-simple-ad-connect/issues",
        "source": "https://github.com/iwouldrathercode/php-simple-ad-connect"
    },
    "require": {
        "php": ">=7.0",
        "phplucidframe/console-table": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^4.1",
        "jakub-onderka/php-console-color": "^0.2.0",
        "clagiordano/phpunit-result-printer": "^2.0"
    },
    "autoload": {
        "psr-4" : {
            "Iwouldrathercode\\SimpleLdap\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
